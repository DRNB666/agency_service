<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.turing.code.tenant.mapper.TenantShopCategoryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.turing.code.tenant.pojo.TenantShopCategory" autoMapping="true">
        <id column="id" property="id" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, sort, status, remark, create_time, update_time
    </sql>
    <select id="selectInCache" resultType="com.turing.code.tenant.pojo.TenantProCategory">
        select
        <include refid="Base_Column_List" />
        from tenant_pro_category
        where status = 1
        order by sort desc
    </select>

</mapper>
